@{
}
@model IEnumerable<YourTour.Models.ViewModels.DiadiemViewModel>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-3 col-sm-6 col-6 destinations-grids text-center">
            <div class="image-position position-relative">
                <a href="#">
                    <img src="/images/tour_images/DaLat.jpg" alt="" class="img-fluid">
                    <p><span class="fa fa-map-signs"></span> <span class="text-uppercase"><b>@item.Tendiadiem</b></span></p>
                </a>
            </div>
        </div>
    }

</div>
